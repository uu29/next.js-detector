{
  "manifest_version": 3,
  "name": "Next.js Detector",
  "version": "1.0.0",
  "description": "웹 페이지가 Next.js로 만들어졌는지 빠르게 확인할 수 있습니다. 아이콘을 클릭하면 현재 페이지의 환경을 자동으로 검사합니다.",
  "author": "uu5474@gmail.com",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "panel.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}